{
  "name": "ArxivFetcher",
  "version": "1.0",
  "description": "Open Arxiv PDFs in KnowPro",
  "manifest_version": 2,
  "author": "Yarani",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon_on_32.png",
    "default_title": "ArxivFetcher"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://arxiv.org/*"
  ]
}