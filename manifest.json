{
  "name": "SciHive",
  "version": "1.1.2",
  "description": "Open Arxiv PDFs in SciHive",
  "manifest_version": 2,
  "author": "Yarani",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon_on_32.png",
    "default_title": "SciHive"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://arxiv.org/*",
    "*://www.arxiv.org/*"
  ]
}
